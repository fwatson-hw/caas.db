<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>CaaS</TargetDatabaseName>
    <DeployScriptFileName>CaaSDB.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=azw-tha-db01.database.windows.net;Persist Security Info=True;User ID=miadmin;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=True</TargetConnectionString>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <ScriptDatabaseOptions>False</ScriptDatabaseOptions>
    <AllowIncompatiblePlatform>True</AllowIncompatiblePlatform>
    <ExcludeLogins>True</ExcludeLogins>
    <IgnoreRoleMembership>True</IgnoreRoleMembership>
    <ExcludeUsers>True</ExcludeUsers>
    <IncludeTransactionalScripts>True</IncludeTransactionalScripts>
    <ProfileVersionNumber>1</ProfileVersionNumber>
  </PropertyGroup>
</Project>